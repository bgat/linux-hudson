#!/usr/bin/make -f
#
# This file was auto-generated by deb-pkg
#

build:
	$(MAKE) ARCH=$(DEB_HOST_GNU_CPU) CROSS_COMPILE=$(DEB_HOST_GNU_TYPE)-

binary-arch:
	$(MAKE) ARCH=$(DEB_HOST_GNU_CPU) CROSS_COMPILE=$(DEB_HOST_GNU_TYPE)- KDEB_SOURCENAME=linux-4.4.35-hudson+ KDEB_PKGVERSION=4.4.35-hudson+-1 bindeb-pkg

clean:
	rm -rf debian/*tmp debian/files
	mv debian/ debian.backup # debian/ might be cleaned away
	$(MAKE) ARCH=$(DEB_HOST_GNU_CPU) clean
	mv debian.backup debian

binary: binary-arch
